!!!
%html
  %head
    %meta{ :charset => "UTF-8" }
    %link{ :rel => "stylesheet", :href => "css/timetable.css" }
    %script{ :src => "js/jquery.js"}
    %script{ :src => "http://base2.googlecode.com/svn/version/1.0/base2-p.js" }
    %script= "base2.JavaScript.bind(window);"
    %script{ :src => "timetable.js" }
    %script{ :src => 'http://gandrew.com/mint/?js' }
    %script{ :src => "timetableData.js" }
  %body
    %div#timetable
      - @timetable.days.each do |day|
        %div.day
          %h2= day
          - @timetable.stages_for_day(day).each do |stage|
            %div.stage
              %h3= stage
              %ul.timetable
                - @timetable.entries_for_day_and_stage(day, stage).each do |entry|
                  - band = entry.band
                  %li.entry{ :id => entry.element_id, :class => "entry" }
                    = haml :_band, :locals => {:b => band, :id => entry.element_id}
    = haml :_analytics
